{
  "compilerOptions": {
    "lib": [
      "es2020",
      "dom"
    ],
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "exactOptionalPropertyTypes": true,
    "baseUrl": ".",
    "types": [
      "@rspack/core/module"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
